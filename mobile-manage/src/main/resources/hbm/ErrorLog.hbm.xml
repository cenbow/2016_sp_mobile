<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.shangpin.wireless.domain">
	<class name="ErrorLog" table="errorLog">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="platform" type="string">
			<column name="platform" length="10"/>
		</property>
		<property name="productNum" type="long">
			<column name="productNum"/>
		</property>
		<property name="productVersion" type="string">
			<column name="productVersion" length="16"/>
		</property>
		<property name="channelNum" type="long">
			<column name="channelNum"/>
		</property>
		<property name="ip" type="string">
			<column name="ip" length="20"/>
		</property>
		<property name="shortmsg" type="string">
			<column name="shortmsg" length="255"/>
		</property>
		<property name="longmsg" type="text">
			<column name="longmsg"/>
		</property>
		<property name="city" type="string">
			<column name="city" length="30"/>
		</property>
		<property name="phoneType" type="string">
			<column name="phoneType" length="20"/>
		</property>
		<property name="imei" type="string">
			<column name="imei" length="40"/>
		</property>
		<property name="systemVersoin" type="string">
			<column name="systemVersoin" length="16"/>
		</property>
		<property name="apn" type="string">
			<column name="apn" length="20"/>
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" type="string">
			<column name="reserve0"/>
		</property>
		<property name="reserve1" type="string">
			<column name="reserve1"/>
		</property>
		<property name="reserve2" type="string">
			<column name="reserve2"/>
		</property>
		<property name="reserve3" type="string">
			<column name="reserve3"/>
		</property>
		<property name="reserve4" type="string">
			<column name="reserve4"/>
		</property>
		<property name="reserve5" type="string">
			<column name="reserve5"/>
		</property>
	</class>
</hibernate-mapping>

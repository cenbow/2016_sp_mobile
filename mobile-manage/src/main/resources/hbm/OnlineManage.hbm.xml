<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.shangpin.wireless.domain">
	<class name="OnlineManage" table="onlineManage">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="productNum" type="long">
			<column name="productNum" />
		</property>
		<property name="channelNum" type="long">
			<column name="channelNum"/>
		</property>
		<property name="versionForceMax" type="string">
			<column name="versionForceMax"  length="16"/>
		</property>
		<property name="versionLatest" type="string">
			<column name="versionLatest" length="16"/>
		</property>
		<property name="fileName" type="string">
			<column name="fileName"/>
		</property>
		<property name="downloadPath" type="string">
			<column name="downloadPath" />
		</property>
		<property name="prompt" type="string">
			<column name="prompt" length="1024" />
		</property>
		<property name="inuse" type="int">
			<column name="inuse" length="4" default="1" />
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" type="string">
			<column name="reserve0"/>
		</property>
		<property name="reserve1" type="string">
			<column name="reserve1"/>
		</property>
		<property name="reserve2" type="string">
			<column name="reserve2"/>
		</property>
		<property name="reserve3" type="string">
			<column name="reserve3"/>
		</property>
		<property name="reserve4" type="string">
			<column name="reserve4"/>
		</property>
		<property name="reserve5" type="string">
			<column name="reserve5"/>
		</property>
	</class>
</hibernate-mapping>

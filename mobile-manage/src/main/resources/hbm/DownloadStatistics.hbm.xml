<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shangpin.wireless.domain">

	<class name="DownloadStatistics" table="downloadstatistics">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="apiname" type="string">
			<column name="apiname" length="100"/>
		</property>
		<property name="visitIp" type="string">
			<column name="visitIp" length="20"/>
		</property>
		<property name="os" type="string">
			<column name="os" length="100"/>
		</property>
		<property name="osv" type="string">
			<column name="osv" length="200"/>
		</property>
		<property name="downloadProductNum" type="long">
			<column name="downProduct"/>
		</property>
		<property name="channelNum" type="long">
			<column name="channelNum"/>
		</property>
		<property name="phoneModel" type="string">
			<column name="phoneModel" length="200"/>
		</property>
		<property name="referer" type="string">
			<column name="referer" length="1024"/>
		</property>
		<property name="params" type="string">
			<column name="params" length="1024"/>
		</property>
		<property name="logTime" type="timestamp">
			<column name="logTime" />
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" />
		<property name="reserve1" />
		<property name="reserve2" />
		<property name="reserve3" />
		<property name="reserve4" />
		<property name="reserve5" />
	</class>
	
</hibernate-mapping>

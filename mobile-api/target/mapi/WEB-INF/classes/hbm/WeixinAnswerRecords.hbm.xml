<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.shangpin.wireless.api.domain">
	<class name="WeixinAnswerRecords" table="weixinanswerrecords">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="weixinId" type="string">
			<column name="weixinId" length="50"/>
		</property>
		<property name="issueId" type="string">
			<column name="issueId" length="20"/>
		</property>
		<property name="questionId" type="string">
			<column name="questionId" length="20"/>
		</property>
		<property name="answer" type="string">
			<column name="answer" length="8"/>
		</property>
		<property name="score" type="int">
			<column name="score" length="10"/>
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" />
		<property name="reserve1" />
		<property name="reserve2" />
		<property name="reserve3" />
		<property name="reserve4" />
		<property name="reserve5" />
	</class>
</hibernate-mapping>

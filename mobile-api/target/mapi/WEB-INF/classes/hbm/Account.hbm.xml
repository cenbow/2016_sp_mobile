<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.shangpin.wireless.api.domain">
	<class name="Account" table="account">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="userId" type="string">
			<column name="userId" length="50"/>
		</property>
		<property name="loginName" type="string">
			<column name="loginName" length="50"/>
		</property>
		<property name="gender" type="string">
			<column name="gender" length="4"/>
		</property>
		<property name="channel" type="long">
			<column name="channel"/>
		</property>
		<property name="product" type="long">
			<column name="product"/>
		</property>
		<property name="regOrigin" type="string">
			<column name="regOrigin" length="10"/>
		</property>
		<property name="platform" type="string">
			<column name="platform" length="10"/>
		</property>
		<property name="phoneType" type="string">
			<column name="phoneType" length="20"/>
		</property>
		<property name="phoneModel" type="string">
			<column name="phoneModel" length="40"/>
		</property>
		<property name="loginTime" type="timestamp">
			<column name="loginTime" />
		</property>
		<property name="regTime" type="timestamp">
			<column name="regTime" />
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" />
		<property name="reserve1" />
		<property name="reserve2" />
		<property name="reserve3" />
		<property name="reserve4" />
		<property name="reserve5" />
	</class>
</hibernate-mapping>

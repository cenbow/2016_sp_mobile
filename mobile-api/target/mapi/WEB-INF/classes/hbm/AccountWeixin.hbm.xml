<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.shangpin.wireless.api.domain">
	<class name="AccountWeixin" table="accountweixin">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="weixinId" type="string">
			<column name="weixinId" length="50"/>
		</property>
		<property name="sex" type="string">
			<column name="sex" length="4"/>
		</property>
		<property name="nickname" type="string">
			<column name="nickname" length="50"/>
		</property>
		<property name="city" type="string">
			<column name="city" length="50"/>
		</property>
		<property name="sceneId" type="string">
			<column name="sceneId" length="12"/>
		</property>
		<property name="subscribeTime" type="timestamp">
			<column name="subscribeTime" />
		</property>
		<property name="unsubscribeTime" type="timestamp">
			<column name="unsubscribeTime" />
		</property>
		<property name="createTime" type="timestamp">
			<column name="createTime" />
		</property>
		<property name="reserve0" />
		<property name="reserve1" />
		<property name="reserve2" />
		<property name="reserve3" />
		<property name="reserve4" />
		<property name="reserve5" />
	</class>
</hibernate-mapping>
